<?xml version="1.0" encoding="UTF-8"?>
<process-application
        xmlns="http://www.camunda.org/schema/1.0/ProcessApplication"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <process-archive name="PQRDS-App">
        <!-- Rutas EXPLÍCITAS: -->
        <resource>static.bpmn/PQRDS.bpmn</resource>
        <resource>static.form/formPQRDS.form</resource>
        <resource>static.form/ArchivarNoCompetencia.form</resource>
        <resource>static.form/AsignarProfesional.form</resource>
        <resource>static.form/NoCompetencia.form</resource>
        <resource>static.form/respuestaFinal.form</resource>
        <resource>static.form/respuestaSolicitud.form</resource>
        <resource>static.form/revisarRespuesta.form</resource>
        <resource>static.form/verificarAsignacion.form</resource>
        <resource>static.form/verificarCompetencia.form</resource>



        <properties>
            <!-- ⚠️ IMPORTANTE: desactivar scanning automático -->
            <property name="isScanForProcessDefinitions">false</property>
            <property name="isDeleteUponUndeploy">true</property>
        </properties>
    </process-archive>

</process-application>